{
  "name": "__MSG_appName__",
  "version": "1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "background":{
    "scripts": ["scripts/events.js"],
    "persistent": false
  },
  "chrome_url_overrides":{
    "newtab": "index.html"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": {
      "19": "icon-16.png",
      "128": "icon-128.png"
    },
    "default_title": "Cloud App"
  },
  "permissions": ["tabs", "storage"]
}
